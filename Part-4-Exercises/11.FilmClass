public class Film {
    
    private String name;
    private int ageRating;

    // Constructor
    public Film(String fileName, int filmAgeRating) {
        this.name = fileName;
        this.ageRating = filmAgeRating;
    }

    // Method to return film name
    public String name() {
        return this.name;
    }

    // Method to return age rating
    public int ageRating() {
        return this.ageRating;
    }
}