// Creates a class in new file
public class Song {

    private String name;
    private int length;

    // Constructor
    public Song(String name, int length) {
        this.name = name;
        this.length = length;
    }

    // Returns variable name
    public String name() {
        return this.name;
    }

    // Returns variable length
    public int length() {
        return this.length;
    }
}